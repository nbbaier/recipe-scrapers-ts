{
  "name": "@recipe-scrapers/typescript",
  "version": "0.1.0",
  "description": "TypeScript port of recipe-scrapers - Extract recipe data from cooking websites",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test",
    "validate-parity": "npm run build && ts-node scripts/validate-parity.ts",
    "compare": "ts-node scripts/compare-outputs.ts",
    "clean": "rm -rf dist coverage *.tsbuildinfo"
  },
  "keywords": [
    "recipe",
    "scraper",
    "parser",
    "cooking",
    "schema.org",
    "structured-data",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hhursev/recipe-scrapers.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/hhursev/recipe-scrapers/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "jsonld": "^8.3.2",
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/cheerio": "^0.22.35",
    "@types/luxon": "^3.3.7",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "chalk": "^4.1.2",
    "diff": "^5.1.0"
  }
}
